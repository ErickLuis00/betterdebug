{
  "name": "betterdebug",
  "displayName": "Better Debug [DEV]",
  "description": "View logs inline without going to browser, similar to console ninja",
  "version": "0.2.0",
  "publisher": "erickluis",
  "engines": {
    "vscode": "^1.96.2"
  },
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/ErickLuis00/betterdebug"
  },
  "categories": [
    "Debuggers",
    "Other"
  ],
  "activationEvents": [
    "onLanguage:typescript",
    "onLanguage:typescriptreact",
    "onLanguage:javascript",
    "onLanguage:javascriptreact"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Better Debug",
      "properties": {
        "betterdebug.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable/disable Better Debug inline logging"
        }
      }
    }
  },
  "scripts": {
    "build": "node ./build.js",
    "package": "pnpm run build && vsce package",
    "publish": "vsce publish",
    "version": "npm version minor",
    "SE_DAR_PROBLEMA_NO_PACKAGE_DE_LIBS_REMOVE_TUDO_DO_DEVEPENDENCIES_DEIXA_SÃ“_DEV_DEPENDENCIES": ""
  },
  "devDependencies": {
    "@babel/helper-module-imports": "^7.25.9",
    "@babel/helper-plugin-utils": "^7.26.5",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.2",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "babel-loader": "^10.0.0",
    "esbuild": "0.25.2",
    "eslint": "^9.23.0",
    "openai": "^4.95.1",
    "typescript": "^5.8.2",
    "ws": "^8.18.1"
  }
}